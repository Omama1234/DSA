/*Given the array arr[] of size n , construct a product Array P such that P[i] is equal to the product
of all the elements of arr except arr[i]
*/
//Microsoft
// Naive approach

#include<bits/stdc++.h>

using namespace std;


int main()
{

    long long  arr[]={10,3,5,6,2};
    int n=5;

    vector<int>v;

    for(int i=0;i<n;i++)
    {
        long long  x=1;
        for(int j=0;j<n;j++)
        {
           if(arr[j]!=arr[i])
           {
               x=x*arr[j];
           }
        }
        //cout<<x<<" ";
        //arr[i]=x;
        v.push_back(x);
    }

    for(int x:v)
    {
        cout<<x<<" ";
    }




}
