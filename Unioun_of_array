/*Unoiun of two array of size n and m*/

#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a[]={89,24,75,11,23};
    int b[]={89,2,4};
    int n=5;
    int m=3;
    set<int>s;
    
    for(int i=0;i<n;i++)
    {
        s.insert(a[i]);
    }
    for(int i=0;i<m;i++)
    {
        s.insert(b[i]);
    }
    
    for(int i:s)
    {
        cout<<i<<" ";
    }

    return 0;
}
