//Naive Approach
// T.C->O(nlogn)
#include<bits/stdc++.h>

using namespace std;

int main()
{
    string s1,s2;
    getline(cin,s1);
    getline(cin,s2);

    sort(s1.begin(),s1.end());
    sort(s2.begin(),s2.end());

    if(s1==s2)
    {
        cout<<"Yes";
    }
    else
    {
        cout<<"No";
    }
    return 0;

}



















//Efficient soltion 
// T.C->O(n+m)

#include<bits/stdc++.h>

using namespace std;

int main()
{
    string s1;
    getline(cin,s1);
    string s2;
    getline(cin,s2);
    int sum1=0;
    int sum2=0;
    for(int i=0;i<s1.size();i++)
    {
        sum1=sum1+(s1[i]-'a');
    }
    for(int i=0;i<s2.size();i++)
    {
        sum2=sum2+(s2[i]-'a');
    }
    if(sum1==sum2)
    {
        cout<<"YES"<<endl;
    }
    else
    {
        cout<<"NO"<<endl;
    }
    return 0;

}
